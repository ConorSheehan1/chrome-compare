{
  "manifest_version": 2,
  "name": "chrome-compare",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js", "url_parser.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage"
  ]

}
